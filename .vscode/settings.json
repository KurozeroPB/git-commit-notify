{
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.tsc.autoDetect": "off",
    "files.associations": {
        ".babelrc": "jsonc",
        ".eslintrc": "jsonc",
        ".prettierrc": "jsonc",
        ".stylelintrc": "json",
        ".dockerignore": "ignore",
        ".prettierignore": "ignore",
        ".eslintignore": "ignore"
    },
    "files.exclude": {
        "out": false
    },
    "search.exclude": {
        ".git": true,
        ".eslintcache": true,
        "dist": true,
        "out": true,
        "node_modules": true,
        "npm-debug.log.*": true,
        "test/**/__snapshots__": true,
        "package-lock.json": true,
        "yarn.lock": true
    }
}
